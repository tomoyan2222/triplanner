<%= render partial: "layouts/errors" %>

<div class="works-login manage">
  <%= form_with url: works_login_path, local: true do |f| %>
    <div class="form_field">
      <%= f.label :user, "ユーザー" %>
      <%= f.text_field :user %>
    </div>
    <div class="form_field">
      <%= f.label :password, "パスワード" %>
      <%= f.password_field :password %>
    </div>
    <div class="form_field">
      <%= f.submit "ログイン" %>
    </div>
  <% end %>
</div>